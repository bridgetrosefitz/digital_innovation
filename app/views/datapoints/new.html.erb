<html>
<%= form_for @datapoint do |f| %>
  <%= f.label :country_id %>
  <%= f.collection_select :country_id, Country.all, :id, :name %><br>
  <%= f.label :indicator_id %>
  <%= f.collection_select :indicator_id, Indicator.all, :id, :name_with_unit %><br>
  <%= f.label :value %>
  <%= f.number_field :value %> 

<% end %>

</html>